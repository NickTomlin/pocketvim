{
  "name": "Pocket Vim",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "This extension Enables vim-mode on sites like Github Gists, JS Bin, CodePen and others",
  "icons": {
    "128": "images/icon.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "html/popup.html"
  },
  "background": {
    "page": "html/background.html"
  },
  "content_scripts": [
    {
       "matches": ["http://jsbin.com/*", "http://cssdeck.com/*", "http://codepen.io/pen/*"],
      "js": [
        "js/lib/require.js",
        "js/lib/jquery.js",
        "js/lib/underscore.js",
        "js/requireContent.js",
        "js/requireConfig.js",
        "js/content.js"
      ]
    }
  ],
  "permissions": [
  ],
  "web_accessible_resources": [
    "js/*",
    "html/*",
    "css/*",
    "images/*"
  ]
}
