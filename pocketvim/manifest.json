{
  "name": "Pocket Vim",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension Enables vim-mode on sites like Github Gists, JS Bin, CodePen and others",
  "icons": {
    "64": "images/icon.png"
  },
  "browser_action": {
    "default_icon": "images/icon.png"
    // "default_popup": "html/popup.html"
  },
  "background": {
    // "page": "html/background.html"
  },
  "content_scripts": [
    {
      "matches": ["*://jsbin.com/*", "*://cssdeck.com/labs/*", "*://codepen.io/pen/*", "*://gist.github.com/*", "*://jsfiddle.net/*", "*://plnkr.co/edit/*"],
      "js": [
        "js/lib/require.js",
        "js/lib/underscore.js",
        "js/requireContent.js",
        "js/requireConfig.js",
        "js/contractor.js"
      ]
    }
  ],
  "permissions": [
  ],
  "web_accessible_resources": [
    "js/*",
    "html/*",
    "css/*",
    "images/*"
  ]
}
